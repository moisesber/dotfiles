[core]
	editor = "vim"
[push]
	default = current
[pull]
	default = current
[color]
	ui = true
[color "diff"]
    meta = yellow bold
[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[merge]
	tool = opendiff
[difftool "sourcetree"]
	cmd = opendiff \"$LOCAL\" \"$REMOTE\"
	path = 
